# Configuração do Banco de Dados
DATABASE_URL=postgresql://usuario:senha@localhost:5432/ubatuba_tourism

# Chave da API OpenAI (para geração de roteiros com IA)
OPENAI_API_KEY=sk-sua_chave_openai_aqui

# Chave secreta para sessões (use uma string longa e aleatória)
SESSION_SECRET=sua_chave_secreta_super_longa_e_aleatoria_aqui

# Ambiente de desenvolvimento
NODE_ENV=development

# Configurações PostgreSQL individuais (opcional)
PGHOST=localhost
PGPORT=5432
PGDATABASE=ubatuba_tourism
PGUSER=postgres
PGPASSWORD=sua_senha_postgres