{
  "name": "ubatuba-tourism-app",
  "version": "1.0.0",
  "description": "Aplicativo de turismo para Ubatuba com IA",
  "main": "dist/index.js",
  "scripts": {
    "dev": "echo \"Use: npm run server:dev (terminal 1) e npm run client:dev (terminal 2)\"",
    "dev:all": "concurrently \"npm run server:dev\" \"wait-on http://localhost:5000 && npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite --config vite.config.local.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --config vite.config.local.ts",
    "build:server": "esbuild server/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --external:pg-native",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "type": "module",
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@neondatabase/serverless": "^0.10.6",
    "@radix-ui/react-accordion": "^1.2.1",
