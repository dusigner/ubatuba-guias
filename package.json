  "scripts": {
    "dev": "concurrently \"NODE_ENV=development tsx server/dev-server.ts\" \"vite dev\"",
    "dev:full": "concurrently \"npm run dev --prefix client\" \"npm run dev --prefix server\"",
    "build": "vite build && npm run build:server",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=functions/dist && rsync -a --exclude='node_modules' server/ functions/",
    "start": "npm run start --prefix client && NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
